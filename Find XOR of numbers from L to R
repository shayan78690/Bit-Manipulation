class Solution {
    public static int findXOR(int l, int r) {
        return (find(l - 1) ^ find(r));
    }
    
    public static int find(int n) {
        int mod = n % 4;
        if(mod == 1) {
            return 1;
        } else if(n == 2) {
            return n+1;
        } else if(n == 3) {
            return 0;
        } 
        return n;
    } 
}
